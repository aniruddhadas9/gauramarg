{
  "name": "gauramarg-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "gcloud-deploy": "gcloud app deploy",
    "gcloud-deploy-no-promoto": "gcloud app deploy --no-promote",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/candifood-ui/stats.json",
    "download-swagger-config-server": "curl https://gauramargrest.appspot.com/api/docs --output src/schema.json",
    "download-swagger-config-local": "curl http://localhost:8080/api/docs --output src/schema.json",
    "download-swagger-ng-lib": "npm run download-swagger-config-local && openapi-generator generate -i src/schema.json -g typescript-angular -o src/app/@restapi --additional-properties=\"ngVersion=9.1.4\"",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.1.0",
    "@angular/animations": "11.0.0",
    "@angular/cdk": "11.0.1",
    "@angular/common": "11.0.0",
    "@angular/compiler": "11.0.0",
    "@angular/core": "11.0.0",
    "@angular/elements": "11.0.0",
    "@angular/forms": "11.0.0",
    "@angular/localize": "11.0.0",
    "@angular/material": "11.0.1",
    "@angular/platform-browser": "11.0.0",
    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/router": "11.0.0",
    "@candiman/website": "10.0.3",
    "@fortawesome/angular-fontawesome": "0.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-regular-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@ngx-formly/bootstrap": "5.6.1",
    "@ngx-formly/core": "5.6.1",
    "angularx-social-login": "2.2.1",
    "bootstrap": "4.4.1",
    "core-js": "3.6.4",
    "crypto-js": "4.0.0",
    "document-register-element": "1.14.3",
    "rxjs": "6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.0",
    "@angular/cli": "11.0.0",
    "@angular/compiler-cli": "11.0.0",
    "@angular/language-service": "11.0.0",
    "@openapitools/openapi-generator-cli": "1.0.12-4.3.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.10",
    "codelyzer": "6.0.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  }
}

    "@openapitools/openapi-generator-cli": "2.2.6",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.54",
    "@types/jasmine": "3.7.0",
    "@types/jasminewd2": "2.0.9",
    "@types/node": "15.0.2 ",

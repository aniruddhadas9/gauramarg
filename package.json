{
  "name": "gauramarg-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "gcloud-deploy": "gcloud app deploy",
    "gcloud-deploy-no-promoto": "gcloud app deploy --no-promote",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/candifood-ui/stats.json",
    "download-config": "curl https://gauramargrest.appspot.com/api/docs --output src/schema.json",
    "download-config-local": "curl http://localhost:8080/api/docs --output src/schema.json",
    "swagger-open-api": "npm run download-config-local && openapi-generator generate -i src/schema.json -g typescript-angular -o src/app/@restapi --additional-properties=\"ngVersion=8.0.0\"",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.7",
    "@angular/animations": "8.2.3",
    "@angular/cdk": "8.1.3",
    "@angular/common": "8.2.3",
    "@angular/compiler": "8.2.3",
    "@angular/core": "8.2.3",
    "@angular/elements": "8.2.3",
    "@angular/forms": "^8.2.3",
    "@angular/material": "8.1.3",
    "@angular/platform-browser": "8.2.3",
    "@angular/platform-browser-dynamic": "8.2.3",
    "@angular/router": "8.2.3",
    "@candiman/website": "0.0.8",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-brands-svg-icons": "5.10.2",
    "@fortawesome/free-regular-svg-icons": "5.10.2",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@ng-bootstrap/ng-bootstrap": "5.1.0",
    "@ngx-formly/bootstrap": "5.4.0",
    "@ngx-formly/core": "5.4.0",
    "angularx-social-login": "1.2.7",
    "bootstrap": "4.3.1",
    "core-js": "3.2.1",
    "crypto-js": "3.1.9-1",
    "document-register-element": "1.14.3",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.0",
    "@angular/cli": "8.3.0",
    "@angular/compiler-cli": "8.2.3",
    "@angular/language-service": "8.2.3",
    "@openapitools/openapi-generator-cli": "0.0.16-4.1.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.2",
    "codelyzer": "5.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",

{
  "name": "gauramarg-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "gcloud-deploy": "gcloud app deploy",
    "gcloud-deploy-no-promoto": "gcloud app deploy --no-promote",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/candifood-ui/stats.json",
    "download-swagger-config-server": "curl https://gauramargrest.appspot.com/api/docs --output src/schema.json",
    "download-swagger-config-local": "curl http://localhost:8080/api/docs --output src/schema.json",
    "download-and-generate-swagger-api-services": "npm run download-swagger-config-server && openapi-generator-cli generate -i src/schema.json -g typescript-angular -o src/app/@restapi --additional-properties=\"ngVersion=11.0.2\"",
    "generate-swagger-api-services": "openapi-generator-cli generate -i src/schema.json -g typescript-angular -o src/app/@restapi --additional-properties=\"ngVersion=11.0.2\"",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "3.0.0-beta.0",
    "@angular/animations": "11.0.2",
    "@angular/cdk": "11.0.1",
    "@angular/common": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/elements": "11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/localize": "11.0.2",
    "@angular/material": "11.0.1",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "@angular/youtube-player": "11.0.1",
    "@candiman/website": "10.0.3",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.2",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ngx-formly/bootstrap": "5.10.19",
    "@ngx-formly/core": "5.10.19",
    "angularx-social-login": "3.5.6",
    "bootstrap": "5.0.0",
    "core-js": "3.12.0",

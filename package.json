{
  "name": "gauramarg-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --aot",
    "gcloud-deploy": "gcloud app deploy",
    "gcloud-deploy-no-promoto": "gcloud app deploy --no-promote",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/candifood-ui/stats.json",
    "download-swagger-config-server": "curl https://gauramargrest.appspot.com/api/docs --output src/schema.json",
    "download-swagger-config-local": "curl http://localhost:8080/api/docs --output src/schema.json",
    "download-swagger-ng-lib": "npm run download-swagger-config-local && openapi-generator generate -i src/schema.json -g typescript-angular -o src/app/@restapi --additional-properties=\"ngVersion=8.2.11\"",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.1.0",
    "@angular/animations": "8.2.11",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.11",
    "@angular/compiler": "8.2.11",
    "@angular/core": "8.2.11",
    "@angular/elements": "8.2.11",
    "@angular/forms": "8.2.11",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.11",
    "@angular/platform-browser-dynamic": "8.2.11",
    "@angular/router": "8.2.11",
    "@candiman/website": "9.0.1",
    "@fortawesome/angular-fontawesome": "0.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-regular-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@ngx-formly/bootstrap": "5.5.12",
    "@ngx-formly/core": "5.5.12",
    "angularx-social-login": "2.2.1",
    "bootstrap": "4.4.1",
    "core-js": "3.6.4",
    "crypto-js": "4.0.0",
    "document-register-element": "1.14.3",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.12",
    "@angular/cli": "8.3.12",
    "@angular/compiler-cli": "8.2.11",
    "@angular/language-service": "8.2.11",
    "@openapitools/openapi-generator-cli": "1.0.10-4.2.3",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.1",
    "codelyzer": "5.1.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.5.3"
  }
